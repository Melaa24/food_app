<% provide(:title, 'All Users') %>
<h1>Customer List</h1>

<div class='row'>
  <%= render 'shared/admin_sidebar' %>
  <div class='span8 well'>
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= link_to user.name, user %>
        </li>
      <% end %>
    </ul>
  </div>
</div>